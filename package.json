{
  "name": "ah-shopify-auth-plugin",
  "version": "0.0.31",
  "description": "Provides shopify authentication to actionhero.js",
  "bugs": {
    "url": "https://github.com/ryangall7/ah-shopify-auth-plugin/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ryangall7/ah-shopify-auth-plugin.git"
  },
  "scripts": {
    "postinstall": "npm run build",
    "dev": "ts-node-dev --no-deps --transpile-only ./src/server.ts",
    "start": "node ./dist/server.js",
    "test": "jest",
    "pretest": "npm run build && npm run lint",
    "build": "tsc --declaration",
    "lint": "prettier --check src/*/** __test__/*/**",
    "pretty": "prettier --write src/*/** __test__/*/**"
  },
  "bundleDependencies": false,
  "author": "Ryan Gallagher",
  "license": "ISC",
  "dependencies": {
    "cookie": "^0.4.1",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "node-fetch": "^2.6.0",
    "nonce": "^1.0.4",
    "querystring": "^0.2.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.30",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.38",
    "@types/validator": "^12.0.1",
    "actionhero": "^21.0.0",
    "jest": "^24.9.0",
    "pg": "^7.18.2",
    "prettier": "^1.19.1",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^24.3.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "peerDependencies": {
    "actionhero": ">=21.0.0"
  },
  "keywords": [
    "shopify",
    "actionhero.js",
    "actionhero",
    "oauth",
    "authentication"
  ]
}
